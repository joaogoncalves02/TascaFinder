{
  "expo": {
    "name": "TascaFinder",
    "slug": "tasca-finder",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "tascafinder",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#0D3B66"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#0D3B66"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-sqlite"
    ],
    "experiments": {
      "tsconfigPaths": true
    }
  }
}
